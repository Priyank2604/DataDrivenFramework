<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Data Driven Basics">
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
        <listener class-name="listeners.CustomListeners"/>
    </listeners>
    <test verbose="2" preserve-order="true" name="LoginTest">
        <classes>
            <class name="TestCases.LoginTest">
                <methods>
                    <include name="loginASBankManager"/>
                </methods>
            </class>
        </classes>
    </test>
    <test verbose="2" preserve-order="true" name="Add Customer Test">
        <classes>
            <class name="TestCases.AddCustomerTest">
            </class>
        </classes>
    </test>
</suite>